	futhark dataset -g i32 --i64-bounds=400:400 -g i64 --i64-bounds=0:3 -g [1000000]i64 --i64-bounds=0:400 -g [1000000]i64 --i64-bounds=0:400 -g [1000000]i64 > data/400_1000000.in

