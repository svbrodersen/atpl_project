datasets:
	mkdir -p data
	futhark dataset -g i32 --i64-bounds=200:200 -g i64 --i64-bounds=0:3 -g [1000]i64 --i64-bounds=0:200 -g [1000]i64 --i64-bounds=0:200 -g [1000]i64 > data/200_1000.in
	futhark dataset -g i32 --i64-bounds=200:200 -g i64 --i64-bounds=0:3 -g [10000]i64 --i64-bounds=0:200 -g [10000]i64 --i64-bounds=0:200 -g [10000]i64 > data/200_10000.in
	futhark dataset -g i32 --i64-bounds=200:200 -g i64 --i64-bounds=0:3 -g [100000]i64 --i64-bounds=0:200 -g [100000]i64 --i64-bounds=0:200 -g [100000]i64  > data/200_100000.in
	futhark dataset -g i32 --i64-bounds=200:200 -g i64 --i64-bounds=0:3 -g [1000000]i64 --i64-bounds=0:200 -g [1000000]i64 --i64-bounds=0:200 -g [1000000]i64 > data/200_1000000.in

	futhark dataset -g i32 --i64-bounds=400:400 -g i64 --i64-bounds=0:3 -g [1000]i64 --i64-bounds=0:400 -g [1000]i64 --i64-bounds=0:400 -g [1000]i64 > data/400_1000.in
	futhark dataset -g i32 --i64-bounds=400:400 -g i64 --i64-bounds=0:3 -g [10000]i64 --i64-bounds=0:400 -g [10000]i64 --i64-bounds=0:400 -g [10000]i64 > data/400_10000.in
